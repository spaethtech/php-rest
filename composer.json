{
    "name": "mvqn/rest",
    "description": "A simple REST Client library for interfacing with generic REST Endpoints.",
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
    ],
    "require": {
        "php": "~7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "mvqn/annotations": "~1",
        "mvqn/collections": "~1",
        "mvqn/common": "~1",
        "mvqn/dynamics": "~1",

        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8",
        "vlucas/phpdotenv": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "MVQN\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MVQN\\": "tests/"
        }
    }
}
