{
    "name": "rspaeth/rest",
    "description": "A simple REST Client library for interfacing with generic REST Endpoints.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
    ],
    "require": {
        "php": "~7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "rspaeth/annotations": "*",
        "rspaeth/collections": "*",
        "rspaeth/common": "*",
        "rspaeth/dynamics": "*",

        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9",
        "vlucas/phpdotenv": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "rspaeth\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rspaeth\\": "./tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        { "type": "path", "url": "../*"}
    ]
}
